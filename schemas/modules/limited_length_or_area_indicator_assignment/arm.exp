END_ENTITY;


ENTITY Ply_drop_off_stay_out_area_llai_specification;
  ply_drop_off_stay_out_area_llai_specification_definition :  Document;
  ply_drop_off_stay_out_area_specification_type :
    ply_drop_off_stay_out_area_llai_specification_type;
END_ENTITY;


ENTITY Surface_preparation_area
  SUBTYPE OF (Area_based_llai);
  surface_preparation_area_specifications: OPTIONAL SET[1:?] OF

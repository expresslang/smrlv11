SCHEMA Annotated_3d_model_equivalence_inspection_result_mim;

  USE FROM Annotated_3d_model_equivalence_criteria_mim;  -- ISO/TS 10303-1838

  USE FROM annotated_3d_model_equivalence_inspection_result_schema;   -- ISO 10303-62

  USE FROM Foundation_representation_mim;   -- ISO/TS 10303-1006
    
  USE FROM product_data_equivalence_inspection_result_schema   -- ISO 10303-62
  (criterion_report_item_with_number_of_instances,
   criterion_report_item_with_value,
   data_equivalence_inspected_element_pair,
   data_equivalence_inspection_criterion_report,
   data_equivalence_inspection_criterion_report_item,
   data_equivalence_inspection_instance_report,
   data_equivalence_inspection_report,
   data_equivalence_inspection_result,
   data_equivalence_inspection_result_with_judgement);
   
   USE FROM shape_data_quality_inspection_result_schema   -- ISO 10303-59
   (boolean_value);

  TYPE a3m_equivalence_measured_value_select = EXTENSIBLE SELECT BASED_ON equivalence_measured_value_select WITH
    (boolean_value);
  END_TYPE;

  TYPE a3mir_accuracy_associated_target_select = EXTENSIBLE SELECT
     BASED_ON accuracy_associated_target_select WITH 
	(a3m_equivalence_inspection_result_representation,
	 data_equivalence_inspection_result);
  END_TYPE;

END_SCHEMA;

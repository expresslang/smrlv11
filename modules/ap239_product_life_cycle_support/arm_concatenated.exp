  WR3: SIZEOF(QUERY(it1 <* SELF\Representation.items | (it1.name <> 'geometry type') AND (SIZEOF(QUERY(it2 <* SELF\Representation.items | it1.name = it2.name)) > 1 )))= 0;
END_ENTITY;

